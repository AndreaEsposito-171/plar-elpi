accumulate "formula".

type p, q, r formula.

atomic P :- std.mem! [p,q,r] P.

main :-
      print "\nTest copy_formula"
    , [ (copy_formula p q :- !)
      , (copy_formula q r :- !)
      , (pi X\ copy_formula X X :- atomic X)
      ] => ok_eq (copy_formula (p && q !! r)) (q && r !! r)
    , print "\nTest onatoms"
    , (pi F\
       [ (F p r :- !)
       , (F r p :- !)
       , (pi X\ F X X)
       ] => ok_eq (onatoms F (p && q !! r)) (r && q !! p))
    , print "\nTest atoms"
    , ok_eq (atoms (p && q !! r)) [p, q, r]
    .

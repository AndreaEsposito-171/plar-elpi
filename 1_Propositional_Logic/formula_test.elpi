accumulate "formula".

type p, q, r formula.

atom P :- std.mem! [p,q,r] P.

main :-
    print "Test 1",
    [(copy_formula p q :- !),
     (copy_formula q r :- !)] =>
    copy_formula (p && q !! r) E1, !, print E1,
    print "Test 2",
    (pi F\
     [F p r, F r p]
     => onatoms F (p && q !! r) E2), !, print E2,
    print "Test 3",
    std.spy(atoms (p && q !! r) E3), !, print E3.
